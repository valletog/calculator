curl --location 'http://localhost:8080/api/v1/calculate' \
--header 'Content-Type: application/json' \
--data '{
  "expression": "2+2*2"
}'
Ответ:
{
  "result": "6.000000"
}

curl --location 'http://localhost:8080/api/v1/calculate' \
--header 'Content-Type: application/json' \
--data '{
  "expression": ""
}'
Ответ:
{
  "error": "Expression cannot be empty"
}

curl --location 'http://localhost:8080/api/v1/calculate' \
--header 'Content-Type: application/json' \
--data '{
  "expression": "2+2a"
}'
Ответ:

{
  "error": "Expression is not valid"
}

curl --location 'http://localhost:8080/api/v1/calculate' \
--header 'Content-Type: application/json' \
--request GET

Ответ:
{
  "error": "Invalid method"
}
